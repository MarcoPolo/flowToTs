{
  "name": "flow-to-ts",
  "version": "1.0.0",
  "description": "Transformer to convert flow files to typescript",
  "main": "",
  "scripts": {
    "test": "jest",
    "transformFlowToTS": "./node_modules/jscodeshift/bin/jscodeshift.js -t src/transformers/flowToTs.ts --transformFileIn=\".js$\" --transformFileOut=\".ts\"",
    "transformFlowToTSX": "./node_modules/jscodeshift/bin/jscodeshift.js -t src/transformers/flowToTs.ts --transformFileIn=\".js$\" --transformFileOut=\".tsx\"",
    "transformFlowDefToDTS": "./node_modules/jscodeshift/bin/jscodeshift.js -t src/transformers/flowToTs.ts --transformFileIn=\".js.flow$\" --transformFileOut=\".d.ts\" --extensions=\"flow\""
  },
  "author": "marco",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/jscodeshift": "^0.6.0",
    "@types/node": "^11.10.4",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "jest": "^24.1.0",
    "jscodeshift": "marcopolo/jscodeshift#support-renaming",
    "ts-jest": "^24.0.0"
  }
}
